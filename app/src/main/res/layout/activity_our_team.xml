<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.dts.finalintegration.OurTeam"
    android:background="#f1eecd">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Our Team"
            android:textStyle="bold"
            android:id="@+id/textView37"
            android:layout_gravity="center_horizontal"
            android:textColor="#060000"
            android:textSize="25dp"
            android:layout_marginBottom="15dp"/>

        <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imageView8"
            android:layout_gravity="center_horizontal"
            android:src="@drawable/ic_group_black_24dp"
            android:scaleType="fitXY"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Mayank Jain"
            android:id="@+id/textView38"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="8dp"
            android:textColor="#0c0000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Sudhanshu Saini"
            android:id="@+id/textView39"
            android:layout_marginBottom="8dp"
            android:layout_gravity="center_horizontal"
            android:textColor="#050000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Gaurav Sachdeva"
            android:id="@+id/textView40"
            android:layout_marginBottom="15dp"
            android:layout_gravity="center_horizontal"
            android:textColor="#070000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Shivam Tandon"
            android:id="@+id/textView42"
            android:layout_marginBottom="8dp"
            android:layout_gravity="center_horizontal"
            android:textColor="#070000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Mayank Raheja"
            android:id="@+id/textView43"
            android:layout_marginBottom="8dp"
            android:layout_gravity="center_horizontal"
            android:textColor="#090000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Naveen Attri"
            android:id="@+id/textView44"
            android:layout_marginBottom="8dp"
            android:layout_gravity="center_horizontal"
            android:textColor="#060000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Devashish Kulbe"
            android:id="@+id/textView45"
            android:layout_gravity="center_horizontal"
            android:textColor="#060000" />
    </LinearLayout>
</RelativeLayout>
